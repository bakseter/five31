        "node": "24.x"
    },
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "format": "prettier -w \"**/*.{ts,tsx,js,jsx,mjs,css,json,md}\"",
